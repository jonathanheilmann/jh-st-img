{
  "name": "jh-st-img",
  "version": "0.0.6",
  "description": "Stencil Lazy Loading Image Component, supporting picture tag with multiple sources.",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@stencil/core": "0.9.5",
    "@stencil/dev-server": "latest",
    "@stencil/sass": "0.0.5",
    "@stencil/utils": "latest",
    "@types/jest": "^21.1.1",
    "jest": "^21.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonathanheilmann/jh-st-img.git"
  },
  "author": "Jonathan heilmann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jonathanheilmann/jh-st-img"
  },
  "homepage": "https://github.com/jonathanheilmann/jh-st-img",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
